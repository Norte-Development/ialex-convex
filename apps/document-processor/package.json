    "form-data": "^4.0.4",
    "gpt-tokenizer": "^3.2.0",
    "ioredis": "^5.8.2",
    "jszip": "^3.10.1",
    "langchain": "^1.0.2",
    "mammoth": "^1.11.0",
    "multer": "2.0.2",
    "node-fetch": "^3.3.2",
