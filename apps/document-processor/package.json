    "bullmq": "^5.31.1",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "gpt-tokenizer": "^2.1.2",
    "ioredis": "^5.4.1",
    "jszip": "^3.10.1",
    "langchain": "^0.3.30",
    "mammoth": "^1.6.0",
    "multer": "1.4.5-lts.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.58.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.10.38",
