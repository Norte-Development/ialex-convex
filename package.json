    "@convex-dev/rag": "^0.3.3",
    "@convex-dev/resend": "^0.1.4",
    "@deepgram/sdk": "^4.11.1",
    "@dmsnell/diff-match-patch": "^1.1.0",
    "@mistralai/mistralai": "^1.7.5",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tiptap/core": "^3.2.1",
    "@tiptap/extension-horizontal-rule": "^3.2.1",
    "@tiptap/extension-text-align": "^3.2.1",
    "@tiptap/extension-text-style": "^3.2.1",
    "@tiptap/extension-underline": "^3.2.1",
    "@tiptap/pm": "^3.2.1",
    "@tiptap/react": "^3.2.1",
    "@tiptap/starter-kit": "^3.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.25.4",
    "firebase-admin": "^13.4.0",
    "jsondiffpatch": "^0.7.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.525.0",
