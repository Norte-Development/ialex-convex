    "next-themes": "^0.4.6",
    "papaparse": "^5.4.1",
    "pdf-lib": "^1.17.1",
    "prosemirror-state": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.3",
    "react-textarea-autosize": "^8.5.9",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5"
