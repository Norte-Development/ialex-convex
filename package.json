    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.0.14",
    "tsx": "^4.19.2",
    "turbo": "^2.2.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^6.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide"
    ],
    "overrides": {
      "@convex-dev/prosemirror-sync>@tiptap/core": "^3.2.1"
    }
  }
