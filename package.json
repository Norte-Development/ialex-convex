{
  "name": "ialex",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=18 <19"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:app:web dev:app:convex",
    "dev:app:web": "pnpm -C apps/application dev:web",
    "dev:app:convex": "pnpm -C apps/application dev:convex",
    "dev:processor": "pnpm -C apps/document-processor dev",
    "dev:all": "npm-run-all --parallel dev dev:processor",
    "build": "turbo run build",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "pnpm -C apps/application preview",
    "ui:add": "pnpm -C apps/application dlx shadcn@latest add",
    "ui": "pnpm -C apps/application dlx shadcn@latest"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.18",
    "@clerk/clerk-react": "^5.25.0",
    "@clerk/react-router": "^1.8.1",
